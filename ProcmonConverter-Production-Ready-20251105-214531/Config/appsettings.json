{
  "Processing": {
    "BatchSize": 25000,
    "MaxConcurrency": 4,
    "TimeoutSeconds": 300,
    "MemoryThresholdMB": 4096
  },
  "Logging": {
    "Level": "Information",
    "EnableFileLogging": true,
    "LogRetentionDays": 30,
    "StructuredLogging": true
  },
  "Health": {
    "EnableHealthChecks": true,
    "HealthCheckIntervalSeconds": 60,
    "MemoryThresholdMB": 1024,
    "DiskThresholdGB": 5
  },
  "Performance": {
    "EnableProfiling": false,
    "EnableCheckpoints": true,
    "CheckpointIntervalMinutes": 5,
    "OptimizeBatchSize": true
  },
  "Patterns": {
    "NetworkPatterns": [
      "TCP",
      "UDP",
      "HTTP",
      "HTTPS",
      "FTP",
      "SMB",
      "DNS",
      "DHCP"
    ],
    "SecurityPatterns": [
      "ACCESS_DENIED",
      "PRIVILEGE",
      "TOKEN",
      "AUTHENTICATION",
      "AUTHORIZATION"
    ],
    "ErrorPatterns": [
      "ERROR",
      "FAIL",
      "EXCEPTION",
      "TIMEOUT",
      "CRASH"
    ],
    "CustomPatterns": {}
  }
}
